    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.14.1/lodash.js" type="text/javascript"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/backbone.js/0.9.2/backbone-min.js" type="text/javascript"></script>

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-bootgrid/1.3.1/jquery.bootgrid.min.css">
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-bootgrid/1.3.1/jquery.bootgrid.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-bootgrid/1.3.1/jquery.bootgrid.fa.min.js"></script>

  <script type="text/javascript" src="https://dl.dropboxusercontent.com/u/57683747/visualization/js/iwc.js"></script>
  <script type="text/javascript" src="https://dl.dropboxusercontent.com/u/57683747/visualization/js/oidc-widget.js"></script>
  <script type="text/javascript" src="https://dl.dropboxusercontent.com/u/57683747/visualization/js/las2peerWidgetLibrary.js"></script>
  <script type="text/javascript" src="https://dl.dropboxusercontent.com/u/57683747/visualization/js/circle-progress.js"></script>
<!--   <script type="text/javascript" src="http://rwth-acis.github.io/Gamification-Visualization-Frontend/js/client.js"></script>  -->
    <script type="text/javascript" src="https://dl.dropboxusercontent.com/u/57683747/visualization/js/gamification-framework.js"></script>
    <script type="text/javascript" src="https://dl.dropboxusercontent.com/u/57683747/visualization/js/vis.js"></script>

 </body>
</html>




  ]]>
  </Content>

</Module>
